<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.reservedcontroller">
   <children>
      <HBox layoutX="18.0" layoutY="76.0" prefHeight="237.0" prefWidth="560.0">
         <children>
            <TableView fx:id="tableView" prefHeight="237.0" prefWidth="560.0">
              <columns>
                  <TableColumn fx:id="seatnum" prefWidth="54.0" text="Seat no." />
                  <TableColumn fx:id="source" prefWidth="81.0" text="Source" />
                <TableColumn fx:id="dest" prefWidth="76.0" text="Destination" />
                  <TableColumn fx:id="internal" prefWidth="65.0" text="Internal" />
                  <TableColumn fx:id="round" prefWidth="69.0" text="Round" visible="false" />
                  <TableColumn fx:id="stops" prefWidth="48.0" text="Stops" />
                  <TableColumn fx:id="vehicle" prefWidth="86.0" text="Vehicle" />
                  <TableColumn fx:id="date" prefWidth="100.0" text="Departure Date" />
                  <TableColumn fx:id="price" prefWidth="49.0" text="Price" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <Label layoutX="235.0" layoutY="28.0" text="Your trips:">
         <font>
            <Font name="System Bold Italic" size="27.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="47.0" layoutY="344.0" mnemonicParsing="false" onAction="#back1" text="Back to Account" />
      <Button fx:id="delete" layoutX="458.0" layoutY="344.0" mnemonicParsing="false" onAction="#delete" text="Delete Selected trip" />
      <Button fx:id="refresh" layoutX="518.0" layoutY="48.0" mnemonicParsing="false" onAction="#show" text="Refresh" />
   </children>
</AnchorPane>
